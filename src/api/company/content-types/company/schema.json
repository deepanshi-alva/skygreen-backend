{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "Company"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "domain": {
      "type": "text"
    },
    "subscription_plan": {
      "type": "enumeration",
      "enum": [
        "FREE",
        "BASIC",
        "PRO",
        "ENTERPRISE"
      ]
    },
    "employers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "company"
    }
  }
}
